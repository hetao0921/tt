file.path=/usr/local/mysql/my.cnf
root.password=root
grant.username=admin
grant.password=111
#slave_skip_errors = 1062
binlog_format=mixed
log_bin=mysql-bin
#binlog-do-db=test1;test2;test3
binlog-do-db=nvmp;gnvmp;jms_client
#binlog-ignore-db=
replicate-ignore-table=nvmp.nvmp_centerinfotab
basedir = /usr/local/mysql
datadir = /usr/local/mysql/data
port = 3306

master.server_id = 1
slave.server_id = 2
auto-increment-increment = 2
master.auto-increment-offset = 2
slave.auto-increment-offset =1