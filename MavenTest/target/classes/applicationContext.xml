    <import resource="classpath:webservice-dao.xml" />
    <!-- webservice发布 -->
    <import resource="classpath:webservice.xml" />
    <!-- service 层 -->
    <!--
    <import resource="classpath:webservice-service.xml" />

    
-->
</beans>
  