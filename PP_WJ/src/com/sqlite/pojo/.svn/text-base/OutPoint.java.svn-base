package com.sqlite.pojo;

public class OutPoint  extends BasePO{
	private int oid;                  //主键ID
	private String deviceId;          //设备ID
	private String deviceName;        //设备名称
	private int channelID;            //通道号
	private int deviceStatus;         //设备状态	 
	
	public OutPoint() {
		super();
	}

	public OutPoint(int oid, String deviceId, String deviceName, int channelID,
			int deviceStatus) {
		super();
		this.oid = oid;
		this.deviceId = deviceId;
		this.deviceName = deviceName;
		this.channelID = channelID;
		this.deviceStatus = deviceStatus;
	}

	public int getOid() {
		return oid;
	}

	public void setOid(int oid) {
		this.oid = oid;
	}

	public String getDeviceId() {
		return deviceId;
	}

	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}

	public String getDeviceName() {
		return deviceName;
	}

	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}

	public int getChannelID() {
		return channelID;
	}

	public void setChannelID(int channelID) {
		this.channelID = channelID;
	}

	public int getDeviceStatus() {
		return deviceStatus;
	}

	public void setDeviceStatus(int deviceStatus) {
		this.deviceStatus = deviceStatus;
	}
	
}
