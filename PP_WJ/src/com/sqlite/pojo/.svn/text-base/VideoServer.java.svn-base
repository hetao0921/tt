package com.sqlite.pojo;

public class VideoServer {

	private int vid;                      //����ID
	private String clientId;              //�ͻ���ID
	private String deviceId;              //�豸ID
	private int devChId;                  //�豸ͨ��ID
	private String videoServerId;         //ת��ID
	private int vSChId;                   //ת��ͨ��ID 
	private int flag;
	private String clientIp;
	private String deviceIp;
	private int linkedMode;               //协议传输模式
	private int lev;                   //级别
	
	public VideoServer() {
		super();
	}

	public VideoServer(int vid, String clientId, String deviceId, int devChId,
			String videoServerId, int vSChId, int flag, String clientIp,
			String deviceIp, int linkedMode, int lev) {
		super();
		this.vid = vid;
		this.clientId = clientId;
		this.deviceId = deviceId;
		this.devChId = devChId;
		this.videoServerId = videoServerId;
		this.vSChId = vSChId;
		this.flag = flag;
		this.clientIp = clientIp;
		this.deviceIp = deviceIp;
		this.linkedMode = linkedMode;
		this.lev = lev;
	}


	public int getVid() {
		return vid;
	}
	public void setVid(int vid) {
		this.vid = vid;
	}
	public String getClientId() {
		return clientId;
	}
	public void setClientId(String clientId) {
		this.clientId = clientId;
	}
	public String getDeviceId() {
		return deviceId;
	}
	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}
	public int getDevChId() {
		return devChId;
	}
	public void setDevChId(int devChId) {
		this.devChId = devChId;
	}
	public String getVideoServerId() {
		return videoServerId;
	}
	public void setVideoServerId(String videoServerId) {
		this.videoServerId = videoServerId;
	}
	public int getvSChId() {
		return vSChId;
	}
	public void setvSChId(int vSChId) {
		this.vSChId = vSChId;
	}
	public int getFlag() {
		return flag;
	}
	public void setFlag(int flag) {
		this.flag = flag;
	}

	public String getClientIp() {
		return clientIp;
	}

	public void setClientIp(String clientIp) {
		this.clientIp = clientIp;
	}

	public String getDeviceIp() {
		return deviceIp;
	}

	public void setDeviceIp(String deviceIp) {
		this.deviceIp = deviceIp;
	}

	public int getLinkedMode() {
		return linkedMode;
	}

	public void setLinkedMode(int linkedMode) {
		this.linkedMode = linkedMode;
	}

	public int getLev() {
		return lev;
	}

	public void setLev(int lev) {
		this.lev = lev;
	}
	
}
