package com.sqlite.pojo;

public class Canmel extends BasePO{

	private int cid;                  //主键ID
	private String deviceId;          //设备ID
	private String deviceName;        //设备名称
	private int channelID;            //通道号
	private int motionStatus;         //移动侦测状态
	private int videoLost;            //视频丢失状态
	
	 
	public Canmel() {
		super();
	}


	public Canmel(int cid, String deviceId, String deviceName, int channelID,
			int motionStatus, int videoLost) {
		super();
		this.cid = cid;
		this.deviceId = deviceId;
		this.deviceName = deviceName;
		this.channelID = channelID;
		this.motionStatus = motionStatus;
		this.videoLost = videoLost;
	}


	public int getCid() {
		return cid;
	}


	public void setCid(int cid) {
		this.cid = cid;
	}


	public String getDeviceId() {
		return deviceId;
	}


	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}


	public String getDeviceName() {
		return deviceName;
	}


	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}


	public int getChannelID() {
		return channelID;
	}


	public void setChannelID(int channelID) {
		this.channelID = channelID;
	}


	public int getMotionStatus() {
		return motionStatus;
	}


	public void setMotionStatus(int motionStatus) {
		this.motionStatus = motionStatus;
	}


	public int getVideoLost() {
		return videoLost;
	}


	public void setVideoLost(int videoLost) {
		this.videoLost = videoLost;
	}
	
	
	
}
