package com.fxdigital.ipmatrix; 

public class VedioSource {
	public String _sourceIp;//视频源IP
	public int _sourcePort;//视频源端口
	public int _sourceChannel;//视频源通道号
	public String get_sourceIp() {
		return _sourceIp;
	}
	public void set_sourceIp(String _sourceIp) {
		this._sourceIp = _sourceIp;
	}
	public int get_sourcePort() {
		return _sourcePort;
	}
	public void set_sourcePort(int _sourcePort) {
		this._sourcePort = _sourcePort;
	}
	public int get_sourceChannel() {
		return _sourceChannel;
	}
	public void set_sourceChannel(int _sourceChannel) {
		this._sourceChannel = _sourceChannel;
	}
	
}
