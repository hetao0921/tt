package NVMP.VideoControlDomain;

public class VideoRoute {
	private String type;
	private String clientid;
	private String clientip;
	private String deviceid;
	private String deviceip;
	private Integer devicechannel;
	private String routemap;
	private Integer lev;
	private String sourceip;
	private Integer sourcechannel;
	private String forwardid;
	private String sourceuuid;
	private String realroutemap;
	private String uuid;
	private Integer sendflag;
	private String sendid;
 
	public String getType() {
		return type;
	}

	/***
	 * 类型有： 1、requestmark 2、fowardmark 3、videoplay
	 * 
	 * */
	public void setType(String type) {
		this.type = type;
	}

	public String getClientid() {
		return clientid;
	}

	public void setClientid(String clientid) {
		this.clientid = clientid;
	}

	public String getClientip() {
		return clientip;
	}

	public void setClientip(String clientip) {
		this.clientip = clientip;
	}

	public String getDeviceid() {
		return deviceid;
	}

	public void setDeviceid(String deviceid) {
		this.deviceid = deviceid;
	}

	public String getDeviceip() {
		return deviceip;
	}

	public void setDeviceip(String deviceip) {
		this.deviceip = deviceip;
	}

	public Integer getDevicechannel() {
		return devicechannel;
	}

	public void setDevicechannel(Integer devicechannel) {
		this.devicechannel = devicechannel;
	}

	public String getRoutemap() {
		if(routemap == null) return "";
		return routemap;
	}

	public void setRoutemap(String routemap) {
		this.routemap = routemap;
	}

	public Integer getLev() {
		return lev;
	}

	public void setLev(Integer lev) {
		this.lev = lev;
	}

	public String getSourceip() {
		if(sourceip==null) return "";
		
		return sourceip;
	}

	public void setSourceip(String sourceip) {
		this.sourceip = sourceip;
	}

	public Integer getSourcechannel() {
		if(sourcechannel==null) return -1;
		return sourcechannel;
	}

	public void setSourcechannel(Integer sourcechannel) {
		this.sourcechannel = sourcechannel;
	}

	public String getForwardid() {
		if (forwardid == null) return ""; 
		return forwardid;
	}

	public void setForwardid(String forwardid) {
		this.forwardid = forwardid;
	}

	public String getSourceuuid() {
		if(sourceuuid == null) return "";
		return sourceuuid;
	}

	public void setSourceuuid(String sourceuuid) {
		this.sourceuuid = sourceuuid;
	}

	public String getRealroutemap() {
		if(realroutemap==null) return "";
		return realroutemap;
	}

	public void setRealroutemap(String realroutemap) {
		this.realroutemap = realroutemap;
	}

	public String getUuid() {
		return uuid;
	}

	public void setUuid(String uuid) {
		this.uuid = uuid;
	}

	public Integer getSendflag() {
		if(sendflag == null) return -1;
		return sendflag;
	}

	public void setSendflag(Integer sendflag) {
		this.sendflag = sendflag;
	}

	public String getSendid() {
		if(sendid == null) return ""; 
		return sendid;
	}

	public void setSendid(String sendid) {
		this.sendid = sendid;
	}

}
