datasource.type=mysql
#datasource.driverClassName=com.oscar.Driver
datasource.driverClassName=com.mysql.jdbc.Driver
#datasource.url=jdbc\:oscar\://192.168.1.169\:2003/nvmp?useUnicode\=true&characterEncoding\=GBK
datasource.url=jdbc\:mysql\://127.0.0.1\:3306/nvmp?useUnicode\=true&characterEncoding\=GBK
datasource.username=admin
datasource.password=111

jndiName=java://comp/env/jdbc/dm
#datasource.password=111

#当连接池中的连接用完时，C3P0一次性创建新连接的数目
c3p0.acquireIncrement=3
#初始化时创建的连接数
c3p0.initialPoolSize=1
#隔多少秒检查所有连接池中的空闲连接
c3p0.idleConnectionTestPeriod=900
#连接池中保留的最小连接数
c3p0.minPoolSize=1
#连接池中保留的最大连接数
c3p0.maxPoolSize=1
#数据源内加载的PreparedStatement数量
c3p0.maxStatements=1
#多线程实现多个操作同时被执行
c3p0.numHelperThreads=10
#最大空闲时间，超过空闲时间的连接将被丢弃
c3p0.maxIdleTime=600

hibernate.dialect=org.hibernate.dialect.MySQLDialect
hibernate.show_sql=true
hiberante.format_sql=true



