package NVMP.AppService.util;

public class ParamInfo {
	String type;
	String name;

	public ParamInfo() {
	};

	public ParamInfo(String a, String b) {
		this.type = a; 
		this.name = b;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	};

}
