package com.sqlite.pojo;

public class CenterNetWork {

	private int id;
	private String netWorkNodeID;            //联网节点 
	private String netWorkNodeIP;            //联网节点IP
	private int netWorkNodePort;             //联网节点端口
	private int isParentFlag;                //是否有上级
	private int isControlFlag;               //是否授权控制
	private String centerId;                 //当前中心ID
	public CenterNetWork() {
		super();
	}
	public CenterNetWork(int id, String netWorkNodeID, String netWorkNodeIP,
			int netWorkNodePort, int isParentFlag, int isControlFlag,
			String centerId) {
		super();
		this.id = id;
		this.netWorkNodeID = netWorkNodeID;
		this.netWorkNodeIP = netWorkNodeIP;
		this.netWorkNodePort = netWorkNodePort;
		this.isParentFlag = isParentFlag;
		this.isControlFlag = isControlFlag;
		this.centerId = centerId;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNetWorkNodeID() {
		return netWorkNodeID;
	}
	public void setNetWorkNodeID(String netWorkNodeID) {
		this.netWorkNodeID = netWorkNodeID;
	}
	public String getNetWorkNodeIP() {
		return netWorkNodeIP;
	}
	public void setNetWorkNodeIP(String netWorkNodeIP) {
		this.netWorkNodeIP = netWorkNodeIP;
	}
	public int getNetWorkNodePort() {
		return netWorkNodePort;
	}
	public void setNetWorkNodePort(int netWorkNodePort) {
		this.netWorkNodePort = netWorkNodePort;
	}
	public int getIsParentFlag() {
		return isParentFlag;
	}
	public void setIsParentFlag(int isParentFlag) {
		this.isParentFlag = isParentFlag;
	}
	public int getIsControlFlag() {
		return isControlFlag;
	}
	public void setIsControlFlag(int isControlFlag) {
		this.isControlFlag = isControlFlag;
	}
	public String getCenterId() {
		return centerId;
	}
	public void setCenterId(String centerId) {
		this.centerId = centerId;
	}
}
