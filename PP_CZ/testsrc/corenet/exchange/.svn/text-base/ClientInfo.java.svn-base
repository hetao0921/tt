package corenet.exchange;

import java.io.Serializable;

public class ClientInfo implements Serializable {

	/**
	 * transient 关键字
	 */
	private static final long serialVersionUID = 0x1001L;

	private String commanderId;//用户ID
	private String deviceId;   //设备ID
	private String groupId;	   //组
	private String deptId;	   //部门
	private String workStatus;
	private boolean isOnline;
	private String workMode;
	private boolean isUpCut;
	private boolean isDownCut;
	private int cameraNum;
	private String userName;
	private String camera1Name;
	private String camera2Name;
	private String camera3Name;
	private String camera4Name;
	private String centerId;
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	public String getCommanderId() {
		return commanderId;
	}
	public String getDeviceId() {
		return deviceId;
	}
	public String getGroupId() {
		return groupId;
	}
	public String getDeptId() {
		return deptId;
	}
	public String getWorkStatus() {
		return workStatus;
	}
	public boolean isOnline() {
		return isOnline;
	}
	public String getWorkMode() {
		return workMode;
	}
	public boolean isUpCut() {
		return isUpCut;
	}
	public boolean isDownCut() {
		return isDownCut;
	}
	public int getCameraNum() {
		return cameraNum;
	}
	public String getUserName() {
		return userName;
	}
	public String getCamera1Name() {
		return camera1Name;
	}
	public String getCamera2Name() {
		return camera2Name;
	}
	public String getCamera3Name() {
		return camera3Name;
	}
	public String getCamera4Name() {
		return camera4Name;
	}
	public String getCenterId() {
		return centerId;
	}
	
	public ClientInfo(String commanderId, String deviceId, String groupId,
			String deptId, String workStatus, boolean isOnline,
			String workMode, boolean isUpCut, boolean isDownCut, int cameraNum,
			String userName, String camera1Name, String camera2Name,
			String camera3Name, String camera4Name, String centerId) {
		this.commanderId = commanderId;
		this.deviceId = deviceId;
		this.groupId = groupId;
		this.deptId = deptId;
		this.workStatus = workStatus;
		this.isOnline = isOnline;
		this.workMode = workMode;
		this.isUpCut = isUpCut;
		this.isDownCut = isDownCut;
		this.cameraNum = cameraNum;
		this.userName = userName;
		this.camera1Name = camera1Name;
		this.camera2Name = camera2Name;
		this.camera3Name = camera3Name;
		this.camera4Name = camera4Name;
		this.centerId = centerId;
	}

	
	
	
	
	
	
}
